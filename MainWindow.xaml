<Window x:Name="Computer_Complector"
	x:Class="ComputerComplectorAdministrativeTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComputerComplectorAdministrativeTool"
        mc:Ignorable="d"
        Title="Computer Complector Administrative Tool" 
        Height="768" 
        Width="1366" 
		MinWidth="800"
        Background="#FF252526" 
        Foreground="White" 
        FontSize="16" 
        WindowState="Maximized" 
        FontFamily="Microsoft Sans Serif" 
        WindowStyle="None">
	<Window.Resources>

		<local:NumericAddSubtractConverter x:Key="NumericConverter"/>
		<local:ArrayElementAtIndexConverter x:Key="ArrayElementAtIndexConverter"/>

		<Storyboard x:Key="LoadingCircle">
			<ThicknessAnimationUsingKeyFrames
                                            Storyboard.TargetProperty="BorderThickness"
                                            Duration="0:0:0.5" RepeatBehavior="Forever">
				<EasingThicknessKeyFrame KeyTime="0%" Value="3,3,0,0"/>
				<EasingThicknessKeyFrame KeyTime="25%" Value="0,3,3,0"/>
				<EasingThicknessKeyFrame KeyTime="50%" Value="0,0,3,3"/>
				<EasingThicknessKeyFrame KeyTime="75%" Value="3,0,0,3"/>
				<EasingThicknessKeyFrame KeyTime="100%" Value="3,3,0,0"/>
			</ThicknessAnimationUsingKeyFrames>
		</Storyboard>

		<!--#region Effects-->

		<DropShadowEffect 
			x:Key="NotHover" 
			ShadowDepth="0" 
			Color="#FFDEFCFF" 
			BlurRadius="10"/>
		<DropShadowEffect 
			x:Key="HoverImg" 
			ShadowDepth="0" 
			Color="#FFDEFCFF" 
			BlurRadius="40"/>
		<DropShadowEffect 
			x:Key="HoverLabel" 
			ShadowDepth="0" 
			Color="#FFDEFCFF" 
			BlurRadius="20"/>

		<DropShadowEffect 
			x:Key="SelectedImg" 
			ShadowDepth="0" 
			Color="#FF46FF00" 
			BlurRadius="40"/>
		<DropShadowEffect 
			x:Key="SelectedItem" 
			ShadowDepth="0" 
			Color="#AA99FF00" 
			BlurRadius="40"/>
		<DropShadowEffect 
			x:Key="SelectedLabel" 
			ShadowDepth="0" 
			Color="#FF46FF00" 
			BlurRadius="20"/>

		<DropShadowEffect 
			x:Key="IncompatibleItem" 
			ShadowDepth="0" 
			Color="#AAFF9900" 
			BlurRadius="40"/>

		<!--#endregion-->

		<!--#region Color Brushes-->

		<SolidColorBrush 
			x:Key="NotSelected" 
			Color="#FF00E8FF"/>
		<SolidColorBrush 
			x:Key="Selected" 
			Color="#FF46FF00"/>

		<!--#endregion-->

		<!--#region Control Templates-->

		<ControlTemplate x:Key="flatListBoxItem" 
			TargetType="ListBoxItem">
			<Border 
				Width="{TemplateBinding Width}"
				Height="{TemplateBinding Height}"
				Background="{TemplateBinding Background}"
				BorderBrush="{TemplateBinding BorderBrush}"
				BorderThickness="{TemplateBinding BorderThickness}">
				<ContentPresenter 
					HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
					VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
					TextElement.Foreground="{TemplateBinding Foreground}"
					TextElement.FontFamily="{TemplateBinding FontFamily}"
					TextElement.FontSize="{TemplateBinding FontSize}"
					TextElement.FontStretch="{TemplateBinding FontStretch}"
					TextElement.FontWeight="{TemplateBinding FontWeight}"/>
			</Border>
		</ControlTemplate>

		<ControlTemplate x:Key="flatBtn" 
			TargetType="Button">
			<Border 
				Width="{TemplateBinding Width}"
				Height="{TemplateBinding Height}"
				Background="{TemplateBinding Background}"
				BorderBrush="{TemplateBinding BorderBrush}"
				BorderThickness="{TemplateBinding BorderThickness}"
				Cursor="Hand">
				<Border.Style>
					<Style TargetType="Border">
						<Style.Setters>
							<Setter Property="Effect" Value="{StaticResource NotHover}"/>
						</Style.Setters>
						<Style.Triggers>
							<Trigger Property="IsMouseOver" Value="True">
								<Trigger.Setters>
									<Setter Property="Effect" Value="{StaticResource HoverImg}"/>
								</Trigger.Setters>
							</Trigger>
						</Style.Triggers>
					</Style>
				</Border.Style>
				<ContentPresenter 
					HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    TextElement.Foreground="{TemplateBinding Foreground}"
                    TextElement.FontFamily="{TemplateBinding FontFamily}"
                    TextElement.FontSize="{TemplateBinding FontSize}"
                    TextElement.FontStretch="{TemplateBinding FontStretch}"
                    TextElement.FontWeight="{TemplateBinding FontWeight}"/>
			</Border>
		</ControlTemplate>

		<ControlTemplate 
			x:Key="flatRoundedBtn" 
			TargetType="Button">
			<Border 
				Width="{TemplateBinding Width}"
				Height="{TemplateBinding Height}"
				Background="{TemplateBinding Background}"
				BorderBrush="{TemplateBinding BorderBrush}"
				BorderThickness="{TemplateBinding BorderThickness}"
				CornerRadius="20">
				<ContentPresenter 
					HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    TextElement.Foreground="{TemplateBinding Foreground}"
                    TextElement.FontFamily="{TemplateBinding FontFamily}"
                    TextElement.FontSize="{TemplateBinding FontSize}"
                    TextElement.FontStretch="{TemplateBinding FontStretch}"
                    TextElement.FontWeight="{TemplateBinding FontWeight}"/>
			</Border>
		</ControlTemplate>

		<ControlTemplate 
			x:Key="flatListViewItem" 
			TargetType="ListViewItem">
			<Border 
				BorderBrush="{TemplateBinding BorderBrush}"
                Background="{TemplateBinding Background}"
                BorderThickness="{TemplateBinding BorderThickness}"
                Focusable="{TemplateBinding Focusable}"
                Height="{TemplateBinding Height}"
                Width="{TemplateBinding Width}"
                HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
                VerticalAlignment="{TemplateBinding VerticalAlignment}">
				<ContentPresenter/>
			</Border>
		</ControlTemplate>

		<ControlTemplate 
			x:Key="headerBtn" 
			TargetType="{x:Type Button}">
			<Grid 
				Background="{TemplateBinding Background}">
				<ContentPresenter 
					x:Name="MyContentPresenter" 
                    Content="{TemplateBinding Content}"
                    HorizontalAlignment="Center" 
                    VerticalAlignment="Center" />
			</Grid>
		</ControlTemplate>

		<!--#endregion-->

		<!--#region Styles-->

		<Style x:Key="elementTextBox"
			TargetType="TextBox">
			<Style.Setters>
				<Setter Property="Background" Value="{x:Null}"/>
				<Setter Property="BorderBrush" Value="{x:Null}"/>
				<Setter Property="VerticalContentAlignment" Value="Center"/>
				<Setter Property="Foreground" Value="White"/>
				<Setter Property="IsReadOnly" Value="True"/>
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate>
							<Border>
								<ScrollViewer x:Name="PART_ContentHost" />
							</Border>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style.Setters>
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">

				</Trigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="panelSelectButtonBorder"
			TargetType="Border">
			<Style.Setters>
				<Setter Property="BorderBrush" Value ="White"/>
				<Setter Property ="BorderThickness" Value="0, 2, 1, 0"/>
				<Setter Property="CornerRadius" Value="10"/>
				<Setter Property="Margin" Value="0, 10, 10, 0"/>
				<Setter Property="Effect" Value="{StaticResource NotHover}"/>
			</Style.Setters>
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Trigger.Setters>
						<Setter Property="Effect" Value="{StaticResource HoverLabel}"/>
					</Trigger.Setters>
				</Trigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="SelectedItemsListView" 
			TargetType="ListViewItem">
			<Style.Setters>
				<Setter 
					Property="BorderThickness" 
					Value="0"/>
				<Setter 
					Property="BorderBrush" 
					Value="{x:Null}"/>
				<Setter 
					Property="Background" 
					Value="{x:Null}"/>
				<Setter 
					Property="Effect" 
					Value="{x:Null}"/>
				<Setter 
					Property="Margin" 
					Value="0,0,0,5"/>
				<Setter 
					Property="Template" 
					Value="{StaticResource flatListBoxItem}"/>
			</Style.Setters>
		</Style>

		<Style x:Key="TabImg" 
			TargetType="Image">
			<Style.Setters>
				<Setter 
					Property="Effect" 
					Value="{StaticResource NotHover}"/>
				<Setter 
					Property="Cursor" 
					Value="Hand"/>
			</Style.Setters>
			<Style.Triggers>
				<Trigger 
					Property="IsMouseOver" 
					Value="True">
					<Trigger.Setters>
						<Setter 
							Property="Effect" 
							Value="{StaticResource HoverImg}"/>
					</Trigger.Setters>
				</Trigger>
				<DataTrigger 
					Binding="{Binding ElementName=BODY_Btn,Path=IsMouseOver,Mode=OneWay}" 
					Value="True">
					<Setter 
						Property="Effect" 
						Value="{StaticResource HoverImg}"/>
				</DataTrigger>
				<DataTrigger 
					Binding="{Binding Path=SelectedElement}" 
					Value="8">
					<Setter 
						Property="Cursor" 
						Value="Arrow"/>
					<Setter 
						Property="Effect" 
						Value="{StaticResource SelectedImg}"/>
				</DataTrigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="elementBtn" 
			TargetType="Button">
			<Style.Setters>
				<Setter 
					Property="Background" 
					Value="#00FFFFFF"/>
				<Setter 
					Property="BorderBrush" 
					Value="#99FFFFFF"/>
				<Setter 
					Property="BorderThickness" 
					Value="3"/>
				<Setter 
					Property="Foreground" 
					Value="White"/>
				<Setter 
					Property="FontSize" 
					Value="32"/>
				<Setter 
					Property="Cursor" 
					Value="Hand"/>
				<Setter 
					Property="Effect" 
					Value="{StaticResource NotHover}"/>
				<Setter 
					Property="Template" 
					Value="{StaticResource flatRoundedBtn}"/>
			</Style.Setters>
			<Style.Triggers>
				<Trigger 
					Property="IsMouseOver" 
					Value="True">
					<Trigger.Setters>
						<Setter 
							Property="Effect" 
							Value="{StaticResource HoverImg}"/>
						<Setter 
							Property="Background" 
							Value="#33FFFFFF"/>
					</Trigger.Setters>
				</Trigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="elementExpanderLabel" 
			TargetType="Label">
			<Style.Setters>
				<Setter 
					Property="Foreground" 
					Value="White"/>
				<Setter 
					Property="FontWeight" 
					Value="Normal"/>
				<Setter 
					Property="FontSize" 
					Value="18"/>
			</Style.Setters>
		</Style>

		<Style x:Key="elementsListLoader" 
			TargetType="Border">
			<Style.Setters>
				<Setter 
					Property="Visibility" 
					Value='Hidden'/>
				<Setter 
					Property="BorderThickness" 
					Value="3,3,0,0"/>
				<Setter 
					Property="Width" 
					Value="120"/>
				<Setter 
					Property="Height" 
					Value="120"/>
				<Setter 
					Property="CornerRadius" 
					Value="60"/>
				<Setter 
					Property="BorderBrush" 
					Value="White"/>
			</Style.Setters>
			<Style.Triggers>
				<DataTrigger 
					Binding="{Binding ElementName=ItemsListBox, Path=Items.Count}" 
					Value="0">
					<DataTrigger.Setters>
						<Setter 
							Property="Visibility" 
							Value="Visible"/>
					</DataTrigger.Setters>
				</DataTrigger>
				<EventTrigger 
					RoutedEvent="Loaded">
					<EventTrigger.Actions>
						<BeginStoryboard 
							Storyboard="{StaticResource LoadingCircle}"/>
					</EventTrigger.Actions>
				</EventTrigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="fieldsListLoader" TargetType="Border">
			<Style.Setters>
				<Setter Property="Visibility" Value='Hidden'/>
				<Setter Property="BorderThickness" Value="3,3,0,0"/>
				<Setter Property="Width" Value="120"/>
				<Setter Property="Height" Value="120"/>
				<Setter Property="CornerRadius" Value="60"/>
				<Setter Property="BorderBrush" Value="White"/>
			</Style.Setters>
			<Style.Triggers>
				<DataTrigger Binding="{Binding ElementName=FiledsListBox, Path=Items.Count}" Value="0">
					<DataTrigger.Setters>
						<Setter Property="Visibility" Value="Visible"/>
					</DataTrigger.Setters>
				</DataTrigger>
				<EventTrigger RoutedEvent="Loaded">
					<EventTrigger.Actions>
						<BeginStoryboard Storyboard="{StaticResource LoadingCircle}"/>
					</EventTrigger.Actions>
				</EventTrigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="listElementBtn" TargetType="Button">
			<Style.Setters>
				<Setter Property="Background" Value="#00FFFFFF"/>
				<Setter Property="BorderBrush" Value="#99FFFFFF"/>
				<Setter Property="BorderThickness" Value="3"/>
				<Setter Property="Foreground" Value="White"/>
				<Setter Property="FontSize" Value="32"/>
				<Setter Property="Cursor" Value="Hand"/>
				<Setter Property="Effect" Value="{StaticResource NotHover}"/>
				<Setter Property="Template" Value="{StaticResource flatRoundedBtn}"/>
				<Setter Property="Margin" Value="15"/>
				<Setter Property="MinWidth" Value="40"/>
				<Setter Property="MinHeight" Value="40"/>
				<Setter Property="HorizontalAlignment" Value="Right"/>
				<Setter Property="VerticalAlignment" Value="Center"/>
			</Style.Setters>
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Trigger.Setters>
						<Setter Property="Effect" Value="{StaticResource HoverImg}"/>
						<Setter Property="Background" Value="#33FFFFFF"/>
					</Trigger.Setters>
				</Trigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="selectElementBtn" TargetType="Button" BasedOn="{StaticResource listElementBtn}">
			<Style.Setters>
				<Setter Property="Command" Value="{Binding ElementName=Computer_Complector, Path=DataContext.SelectItem}"/>
				<Setter Property="CommandParameter" Value="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"/>
			</Style.Setters>
		</Style>

		<Style x:Key="deleteElementBtn" TargetType="Button" BasedOn="{StaticResource selectElementBtn}">
			<Style.Setters>
				<Setter Property="Visibility" Value="Visible"/>
				<Setter Property="Command" Value="{Binding ElementName=Computer_Complector, Path=DataContext.DeleteElement}"/>
				<Setter Property="CommandParameter" Value="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"/>
			</Style.Setters>
		</Style>

		<Style x:Key="editElementBtn" TargetType="Button" BasedOn="{StaticResource selectElementBtn}">
			<Style.Setters>
				<Setter Property="Visibility" Value="Visible"/>
			</Style.Setters>
		</Style>

		<Style x:Key="selectedViewList" TargetType="ListView">
			<Style.Setters>
				<Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Hidden"/>
				<Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Hidden"/>
				<Setter Property="Background" Value="{x:Null}"/>
				<Setter Property="BorderBrush" Value="{x:Null}"/>
				<Setter Property="Foreground" Value="{x:Null}"/>
				<Setter Property="ItemContainerStyle" Value="{StaticResource SelectedItemsListView}"/>
			</Style.Setters>
		</Style>

		<Style x:Key="selectedListViewContainer" TargetType="ListView">
			<Style.Setters>
				<Setter Property="DataContext" Value="{Binding Selected}"/>
				<Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Hidden"/>
				<Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Hidden"/>
				<Setter Property="Background" Value="{x:Null}"/>
				<Setter Property="BorderBrush" Value="{x:Null}"/>
				<Setter Property="Foreground" Value="{x:Null}"/>
				<Setter Property="ItemContainerStyle" Value="{StaticResource SelectedItemsListView}"/>
				<Setter Property="ScrollViewer.CanContentScroll" Value="False"/>
			</Style.Setters>
		</Style>

		<Style x:Key="selectedItemDeselectBtn" TargetType="Button">
			<Style.Setters>
				<Setter Property="Background" Value="#00FFFFFF"/>
				<Setter Property="BorderBrush" Value="#99FFFFFF"/>
				<Setter Property="BorderThickness" Value="3"/>
				<Setter Property="Foreground" Value="White"/>
				<Setter Property="FontSize" Value="32"/>
				<Setter Property="Cursor" Value="Hand"/>
				<Setter Property="Effect" Value="{StaticResource NotHover}"/>
				<Setter Property="Template" Value="{StaticResource flatRoundedBtn}"/>
				<Setter Property="Margin" Value="20"/>
				<Setter Property="MinWidth" Value="40"/>
				<Setter Property="MinHeight" Value="40"/>
				<Setter Property="HorizontalAlignment" Value="Right"/>
				<Setter Property="VerticalAlignment" Value="Center"/>
				<Setter Property="Command" Value="{Binding ElementName=Computer_Complector, Path=DataContext.DeselectItem}"/>
				<Setter Property="CommandParameter" Value="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"/>
			</Style.Setters>
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Trigger.Setters>
						<Setter Property="Effect" Value="{StaticResource HoverImg}"/>
						<Setter Property="Background" Value="#33FFFFFF"/>
					</Trigger.Setters>
				</Trigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="minimizeBtn" TargetType="Button">
			<Style.Setters>
				<Setter Property="Background" Value="#FF1E1E1E"/>
				<Setter Property="Foreground" Value="White"/>
				<Setter Property="Margin" Value="10,0,10,5"/>
				<Setter Property="BorderBrush" Value="{x:Null}"/>
				<Setter Property="FontWeight" Value="Bold"/>
				<Setter Property="Template" Value="{StaticResource headerBtn}"/>
			</Style.Setters>
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Trigger.Setters>
						<Setter Property="Background" Value="#FF3C3C3C"/>
					</Trigger.Setters>
				</Trigger>
				<Trigger Property="IsPressed" Value="True">
					<Trigger.Setters>
						<Setter Property="Background" Value="#FF5A5A5A"/>
					</Trigger.Setters>
				</Trigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="restoreBtn" TargetType="Button">
			<Style.Setters>
				<Setter Property="Background" Value="#FF1E1E1E"/>
				<Setter Property="Foreground" Value="White"/>
				<Setter Property="Template" Value="{StaticResource headerBtn}"/>
				<Setter Property="Margin" Value="10,0,10,5"/>
				<Setter Property="BorderBrush" Value="{x:Null}"/>
				<Setter Property="FontWeight" Value="Bold"/>
			</Style.Setters>
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Trigger.Setters>
						<Setter Property="Background" Value="#FF3C3C3C"/>
					</Trigger.Setters>
				</Trigger>
				<Trigger Property="IsPressed" Value="True">
					<Trigger.Setters>
						<Setter Property="Background" Value="#FF5A5A5A"/>
					</Trigger.Setters>
				</Trigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="closeBtn" TargetType="Button">
			<Style.Setters>
				<Setter Property="Background" Value="#FF5A1E1E"/>
				<Setter Property="Foreground" Value="White"/>
				<Setter Property="Margin" Value="10,0,10,5"/>
				<Setter Property="BorderBrush" Value="{x:Null}"/>
				<Setter Property="FontWeight" Value="Bold"/>
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate TargetType="{x:Type Button}">
							<Grid Background="{TemplateBinding Background}">
								<ContentPresenter x:Name="MyContentPresenter" 
                                          Content="{TemplateBinding Content}"
                                          HorizontalAlignment="Center" 
                                          VerticalAlignment="Center" />
							</Grid>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style.Setters>
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Trigger.Setters>
						<Setter Property="Background" Value="#FF781E1E"/>
					</Trigger.Setters>
				</Trigger>
				<Trigger Property="IsPressed" Value="True">
					<Trigger.Setters>
						<Setter Property="Background" Value="#FF961E1E"/>
					</Trigger.Setters>
				</Trigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="headerLabel" TargetType="Label">
			<Style.Setters>
				<Setter Property="Foreground" Value="White"/>
				<Setter Property="Margin" Value="20,0,0,0"/>
				<Setter Property="VerticalAlignment" Value="Center"/>
				<Setter Property="FontWeight" Value="Bold"/>
			</Style.Setters>
		</Style>

		<Style x:Key="loginBtn" TargetType="Button">
			<Style.Setters>
				<Setter Property="Background" Value="{x:Null}"/>
				<Setter Property="Foreground" Value="White"/>
				<Setter Property="BorderBrush" Value="{x:Null}"/>
				<Setter Property="Margin" Value="3"/>
				<Setter Property="Background" Value="#FF2D2D30"/>
				<Setter Property="Content" Value="Login"/>
				<Setter Property="HorizontalContentAlignment" Value="Center"/>
				<Setter Property="VerticalContentAlignment" Value="Center"/>
				<Setter Property="Template" Value="{StaticResource flatBtn}"/>
			</Style.Setters>
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Trigger.Setters>
						<Setter Property="Background" Value="#FF3C3C3C"/>
					</Trigger.Setters>
				</Trigger>
				<Trigger Property="IsPressed" Value="True">
					<Trigger.Setters>
						<Setter Property="Background" Value="#FF5A5A5A"/>
					</Trigger.Setters>
				</Trigger>
			</Style.Triggers>
		</Style>

		<!--#endregion-->

		<!--#region Data Templates-->

		<!--#region Elements-->

		<DataTemplate x:Key="BODYElements">
			<Border BorderBrush="White" MinWidth="600" MinHeight="100" MaxWidth="{Binding ElementName='ItemsListBox',Path='ActualWidth'}" BorderThickness="1" CornerRadius="5" ScrollViewer.VerticalScrollBarVisibility="Hidden" HorizontalAlignment="Stretch">
				<Grid MinWidth="600" MinHeight="100">
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="3*"/>
						<ColumnDefinition Width="6*"/>
						<ColumnDefinition Width="3*"/>
					</Grid.ColumnDefinitions>
					<Grid.RowDefinitions>
						<RowDefinition Height="1*"/>
						<RowDefinition Height="4*"/>
					</Grid.RowDefinitions>
					<TextBlock MaxWidth="{Binding ElementName='ItemsListBox',Path='ActualWidth'}" TextWrapping="WrapWithOverflow" Foreground="White" FontSize="20" Grid.Column="1" Margin="0,10,10,0" HorizontalAlignment="Stretch" Text="{Binding Path=Title}" VerticalAlignment="Center"/>
					<Image Grid.Row="0" Grid.RowSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Source="Resources/Images/BODY.png" Margin="23,10,23.4,10.8" MaxWidth="120" MaxHeight="120"/>
					<StackPanel Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="1" Margin="0,0,0,20">
						<Separator Background="White" MinHeight="2"/>
						<StackPanel Orientation="Horizontal" Margin="0">
							<Label Foreground="White" VerticalAlignment="Center" Content="Formfactor:"/>
							<Label Foreground="White" FontSize="20" VerticalAlignment="Center" Margin="0,0,20,0" Content="{Binding Path=Formfactor}"/>
							<Label Foreground="White" VerticalAlignment="Center" Content="Type:"/>
							<Label Foreground="White" FontSize="20" VerticalAlignment="Center" Margin="0,0,20,0" Content="{Binding Path=Type}"/>
							<Label Foreground="White" VerticalAlignment="Center" Content="USB ports:"/>
							<Label Foreground="White" FontSize="20" VerticalAlignment="Center" Margin="0,0,20,0" Content="{Binding Path=USBPorts}"/>
						</StackPanel>
						<Expander Foreground="White" FontWeight="Bold" FontSize="20" Margin="0" Expanded="Expander_Expanded">
							<Expander.Header>
								Info
							</Expander.Header>
							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="1*"/>
									<ColumnDefinition Width="2*"/>
								</Grid.ColumnDefinitions>
								<Grid.RowDefinitions>
									<RowDefinition Height="1*"/>
									<RowDefinition Height="1*"/>
									<RowDefinition Height="1*"/>
									<RowDefinition Height="1*"/>
									<RowDefinition Height="1*"/>
									<RowDefinition Height="1*"/>
									<RowDefinition Height="1*"/>
									<RowDefinition Height="1*"/>
									<RowDefinition Height="1*"/>
									<RowDefinition Height="1*"/>
									<RowDefinition Height="1*"/>
									<RowDefinition Height="1*"/>
									<RowDefinition Height="1*"/>
								</Grid.RowDefinitions>
								<Label Style="{StaticResource elementExpanderLabel}" Grid.Column="0" Grid.Row="0" Content="Company:"/>
								<TextBox Grid.Column="1" Grid.Row="0" Text="{Binding Path=Company, Mode=TwoWay}">
									<TextBox.Style>
										<Style TargetType="TextBox">
